@model Schedule.Models.ScheduleTable

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Вы действительно хотите удалить это?</h3>
    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Время
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.CallSheduleId)
            </dd>
    
            <dt>
               № пары
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ParaId)
            </dd>
    
            <dt>
                Аудитория
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.RoomId)
            </dd>
    
            <dt>
                Дисциплина
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.SubjectId)
            </dd>
    
            <dt>
               Группа
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.GroupId)
            </dd>
    
            <dt>
                Преподаватель
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.TeacherId)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Удалить" class="btn btn-primary" /> |
                @Html.ActionLink("Назад", "Index")
            </div>
        }
    </div>
</body>
</html>
