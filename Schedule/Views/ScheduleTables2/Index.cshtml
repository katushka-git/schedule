@model IEnumerable<Schedule.Models.ScheduleTable>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Добавить пару", "Create")
</p>
<table class="table">
    <tr>
        <th>
            № п/п
        </th>
        <th>
           День
        </th>
        <th>
            Группа
        </th>
         <th>
            Аудитория
        </th>
        <th>
           Дисциплина
        </th>
        <th>
            Преподаватель
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CallShedule.NamberPar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Day.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Group.Number)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Room.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Teacher.FIO)
        </td>
        <td>
            @Html.ActionLink("Изменить", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Подробнее", "Details", new { id=item.Id }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
